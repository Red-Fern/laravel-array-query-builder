{
    "name": "red-fern/laravel-array-query-builder",
    "description": "Query an eloquent model using arrays",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Wilson",
            "email": "adam@red-fern.co.uk"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "5.7.* || 5.8.* || ^6.0",
        "illuminate/support": "5.7.* || 5.8.* || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "3.7.* || 3.8.* || ^4.0",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "RedFern\\ArrayQueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedFern\\ArrayQueryBuilder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedFern\\ArrayQueryBuilder\\LaravelArrayQueryBuilderServiceProvider"
            ]
        }
    }
}
